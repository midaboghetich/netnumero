<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui">

    <gwt:HTMLPanel>
        <table class="inh-add-inline" height="24px" cellpadding="0" cellspacing="0" width="100%">
            <tbody>
                <tr>
                    <td>
                        <gwt:SuggestBox ui:field="suggestBox"/>
                    </td>
                    <td>
                        <gwt:Anchor ui:field='btSearch'>
                            <img src="../../i/common/16/search.png" border="0"/>
                        </gwt:Anchor>
                    </td>
                    <td>
                        <gwt:Anchor ui:field='btAdd'>
                            <img src="../../i/common/16/add2.png" border="0"/>
                        </gwt:Anchor>
                    </td>
                </tr>
            </tbody>
        </table>
    </gwt:HTMLPanel>

</ui:UiBinder>
