/* Calendar
-------------------------------------------------*/

.CalendarWidget {
    border: 1px solid #ACA899;
}

.CalendarWidget .navbar .header {
    width: 100%;
    background: #C3D9FF;
    vertical-align: middle;
    border-bottom: 1px solid #ACA899;
    height: auto;
}

.CalendarWidget .navbar .gwt-Button {
    padding-left: 5px;
    padding-right: 5px;
}

.CalendarWidget .table {
    font: 10pt sans-serif;
    text-align: center;
}

.CalendarWidget .weekheader {
    background-color: #ACA899;
}

.CalendarWidget .weekheader .days {
    width: 3em;
}

.CalendarWidget .cell {
    cursor: pointer;
}

.CalendarWidget .cell .gwt-HTML {
    border: 1px solid #ACA899;
}

.CalendarWidget .cell .gwt-HTML span {
    width: 100%;
    height: 100%;
    line-height: 2em;
}

.CalendarWidget .today .gwt-HTML {
    background-color: #C3D9FF;
}

.CalendarWidget .day .gwt-HTML {
    border: 1px solid #C3D9FF;
}

/* validation
-------------------------------------------------*/
.validation-advice {
    text-align: center;
    border: 1px solid #ebb9bc;
    background-color: #ffd5d6;
    font-family: "CalibriRegular", sans-serif;
    font-size: 14px;
    color: #000000;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    margin: 10 0 10 0;
}

.validation-advice a.header {
    font-family: "CalibriRegular", sans-serif;
    font-size: 14px;
    color: #000000;
    text-decoration: none;
}

input.validation-failed, textarea.validation-failed {
    background-color: #ffd5d6;
}

.error {
    font-family: "CalibriRegular", sans-serif;
    font-size: 14px;
    color: #000000;
}

