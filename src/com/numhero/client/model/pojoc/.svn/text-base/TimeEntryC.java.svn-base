package com.numhero.client.model.pojoc;

import com.numhero.shared.pojoc.BasePojoC;

import java.util.Date;

public class TimeEntryC extends BasePojoC {

    private static final String DATE = "date";
    private static final String TASK_ID = "taskId";
    private static final String PROJECT_ID = "projectId";
    private static final String TASK_NAME = "taskName";
    private static final String PROJECT_NAME = "projectName";
    private static final String DESCRIPTION = "description";
    private static final String CREATED = "created";
    private static final String NOTES = "notes";
    private static final String TIME_UNIT = "timeUnit";


    public TimeEntryC() {
    }

    public Date getDate() {
        return getAttributeAsDate(DATE);
    }

    public void setDate(Date created) {
        setAttribute(DATE, created);
    }

    public Date getCreated() {
        return getAttributeAsDate(CREATED);
    }

    public void setCreated(Date created) {
        setAttribute(CREATED, created);
    }

    public String getTaskId() {
        return getAttributeAsString(TASK_ID);
    }

    public void setTaskId(String taskId) {
        setAttribute(TASK_ID, taskId);
    }

    public String getProjectId() {
        return getAttributeAsString(PROJECT_ID);
    }

    public void setProjectId(String projectId) {
        setAttribute(PROJECT_ID, projectId);
    }

    public String getDescription() {
        return getAttributeAsString(DESCRIPTION);
    }

    public void setDescription(String description) {
        setAttribute(DESCRIPTION, description);
    }

    public String getNotes() {
        return getAttributeAsString(NOTES);
    }

    public void setNotes(String description) {
        setAttribute(NOTES, description);
    }


    public String getProjectName() {
        return getAttributeAsString(PROJECT_NAME);
    }

    public void setProjectName(String description) {
        setAttribute(PROJECT_NAME, description);
    }

    public String getTaskName() {
        return getAttributeAsString(TASK_NAME);
    }

    public void setTaskName(String description) {
        setAttribute(TASK_NAME, description);
    }

    public Double getTimeUnit() {
        return getAttributeAsDouble(TIME_UNIT);
    }

    public void setTimeUnit(Double timeUnit) {
        setAttribute(TIME_UNIT, timeUnit);
    }
}