<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
    <ui:style>
        .important {
        font-weight: bold;
        }
        .documentRow p {
        margin:0;
        padding:0;
        }
    </ui:style>

    <div class="{style.documentRow}">
        <a ui:field="editInvoice"/>
        <span class="{style.important}" ui:field="observations"/>
        <span class="{style.important}" ui:field="status"/>
    </div>

</ui:UiBinder>
