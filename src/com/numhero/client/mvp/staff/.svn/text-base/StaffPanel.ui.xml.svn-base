<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:nhw="urn:import:com.numhero.client.widget"
             xmlns:gwt="urn:import:com.google.gwt.user.client.ui" ui:defaultLocale="en"
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateFilename="Staff" ui:generateLocales="default">

    <gwt:VerticalPanel>

        <gwt:DisclosurePanel ui:field="errorsPanel" styleName="validation-advice" width="100%">
            <gwt:header>Errors</gwt:header>
        </gwt:DisclosurePanel>

        <gwt:HTMLPanel>
            <table class="formContainer" cellpadding="0" cellspacing="0"
                   width="100%">
                <thead>
                    <tr>
                        <td>
                            <ui:msg key="staff.contact.information">Contact information</ui:msg>
                        </td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.name">Name</ui:msg>
                                </gwt:Label>
                                <gwt:TextBox ui:field="name"/>
                            </nhw:FormItem>
                        </td>
                    </tr>

                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.email">Email</ui:msg>
                                </gwt:Label>
                                <gwt:TextBox ui:field="tbEmail"/>
                            </nhw:FormItem>
                        </td>
                    </tr>
                    <tr>
                        <td class="formItem">
                            <table cellpadding="0" cellspacing="0" width="100%">
                                <tr>
                                    <td width="60%">
                                        <nhw:FormItem titleWidth="200">
                                            <gwt:Label>
                                                <ui:msg key="staff.phone">Phone</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbPhone"/>
                                        </nhw:FormItem>
                                    </td>
                                    <td width="40%">
                                        <nhw:FormItem titleWidth="60">
                                            <gwt:Label>
                                                <ui:msg key="staff.mobile">Mobile</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbMobile"/>
                                        </nhw:FormItem>
                                    </td>
                                    <td width="40%">
                                        <nhw:FormItem titleWidth="60">
                                            <gwt:Label>
                                                <ui:msg key="staff.fax">Fax</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbFax"/>
                                        </nhw:FormItem>
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </tbody>
            </table>
        </gwt:HTMLPanel>


        <gwt:HTMLPanel>
            <table class="formContainer" cellpadding="0" cellspacing="0"
                   width="100%">
                <thead>
                    <tr>
                        <td>
                            <ui:msg key="staff.address.title">Address</ui:msg>
                        </td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.address">Address</ui:msg>
                                </gwt:Label>
                                <gwt:TextBox ui:field="tbAddress"/>
                            </nhw:FormItem>
                        </td>
                    </tr>
                    <tr>
                        <td class="formItem">
                            <table cellpadding="0" cellspacing="0" width="100%">
                                <tr>
                                    <td width="40%">
                                        <nhw:FormItem titleWidth="200">
                                            <gwt:Label>
                                                <ui:msg key="staff.city">City</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbCity"/>
                                        </nhw:FormItem>
                                    </td>
                                    <td width="30%">
                                        <nhw:FormItem titleWidth="75" titleAlign="ALIGN_CENTER">
                                            <gwt:Label>
                                                <ui:msg key="staff.region">Region</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbRegion"/>
                                        </nhw:FormItem>
                                    </td>
                                    <td width="30%">
                                        <nhw:FormItem titleWidth="75" titleAlign="ALIGN_CENTER">
                                            <gwt:Label>
                                                <ui:msg key="staff.zip.code">Zip Code</ui:msg>
                                            </gwt:Label>
                                            <gwt:TextBox ui:field="tbZipCode"/>
                                        </nhw:FormItem>
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.country">Country</ui:msg>
                                </gwt:Label>
                                <gwt:TextBox ui:field="tbCountry"/>
                            </nhw:FormItem>
                        </td>
                    </tr>
                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.internal.notes">Internal notes</ui:msg>
                                </gwt:Label>
                                <gwt:TextArea ui:field="taNotes"/>
                            </nhw:FormItem>
                        </td>
                    </tr>

                    <tr>
                        <td class="formItem">
                            <nhw:FormItem titleWidth="200">
                                <gwt:Label>
                                    <ui:msg key="staff.billing.rate">Billing Rate</ui:msg>
                                </gwt:Label>
                                <gwt:TextArea ui:field="billingRate"/>
                            </nhw:FormItem>
                        </td>
                    </tr>


                </tbody>
            </table>
        </gwt:HTMLPanel>

        <gwt:HorizontalPanel styleName="buttonsPanel">

            <nhw:NetNumeroButton styleName="inh-button" href="javascript:;" ui:field="btSave">
                <ui:msg key="staff.save">Save</ui:msg>
            </nhw:NetNumeroButton>

            <nhw:NetNumeroButton styleName="inh-button" href="javascript:;" ui:field="btCancel">
                <ui:msg key="staff.cancel">Cancel</ui:msg>
            </nhw:NetNumeroButton>

        </gwt:HorizontalPanel>
    </gwt:VerticalPanel>

</ui:UiBinder>
